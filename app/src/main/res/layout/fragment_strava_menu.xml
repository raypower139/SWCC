<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.MenuFragment">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="88dp"
        android:layout_marginTop="164dp"
        android:text="Record or View Results"
        android:textAlignment="center"
        android:textAllCaps="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/strava_activities_list" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="161dp"
        android:layout_height="73dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/strava_activities_list"
        android:layout_width="188dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:background="@color/colorPrimary"
        android:text="Latest Activities"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/buttonGroupThree"
        app:layout_constraintTop_toBottomOf="@+id/buttonGroupThree" />

    <Button
        android:id="@+id/buttonGroupOne"
        android:layout_width="79dp"
        android:layout_height="40dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="12dp"
        android:background="@color/colorPrimary"
        android:text="Group 1"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
