        app:layout_constraintTop_toBottomOf="@+id/strava_menu_choose_group_text" />

    <Button
        android:id="@+id/buttonGroupTwo"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginStart="30dp"
        android:background="@color/colorPrimary"
        android:text="Group 2"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/buttonGroupOne"
        app:layout_constraintTop_toTopOf="@+id/buttonGroupOne" />

    <Button
        android:id="@+id/buttonGroupThree"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:layout_marginTop="12dp"
        android:background="@color/colorPrimary"
        android:text="Group 3"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/buttonGroupOne"
        app:layout_constraintTop_toBottomOf="@+id/buttonGroupOne" />

    <Button
        android:id="@+id/buttonGroupFour"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:background="@color/colorPrimary"
        android:text="Group 4"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/buttonGroupTwo"
        app:layout_constraintTop_toTopOf="@+id/buttonGroupThree" />

    <TextView
        android:id="@+id/strava_menu_choose_group_text"
        android:layout_width="286dp"
        android:layout_height="24dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="36dp"
        android:text="View Members of Each Challenge"
        android:textAlignment="center"
        android:textSize="18dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="64dp"
        android:text="Mahon Falls"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/strava_activities_list" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:text="Seskin Hill"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:text="Mt.Leinster"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/strava_segment_button"
        android:layout_width="110dp"
        android:layout_height="25dp"
        android:layout_marginStart="4dp"
        android:background="@color/colorPrimary"
        android:text="Leaderboard"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/my_efforts_button_MahonFalls"
