    <Button
        android:id="@+id/strava_segment_button_Mt_Leinster"
        android:layout_width="110dp"
        android:layout_height="25dp"
        android:background="@color/colorPrimary"
        android:text="Leaderboard"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/strava_segment_button_SeskinHill"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <Button
        android:id="@+id/my_efforts_button_MahonFalls"
        android:layout_width="80dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:background="@color/colorPrimary"
        android:text="Check"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <Button
        android:id="@+id/my_efforts_button_SeskinHill"
        android:layout_width="80dp"
        android:layout_height="25dp"
        android:background="@color/colorPrimary"
        android:text="Check"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/my_efforts_button_MahonFalls"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <Button
        android:id="@+id/my_efforts_button_Mt_Leinster"
        android:layout_width="80dp"
        android:layout_height="25dp"
        android:background="@color/colorPrimary"
        android:text="Check"
        android:textColor="@color/colorWhite"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/my_efforts_button_SeskinHill"
        app:layout_constraintTop_toTopOf="@+id/textView9" />

    <Button
        android:id="@+id/addClimbs"
        android:layout_width="109dp"
        android:layout_height="51dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="56dp"
        android:text="Record"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <Button
        android:id="@+id/button_strava_view_climbs"
        android:layout_width="124dp"
        android:layout_height="49dp"
        android:layout_marginStart="20dp"
        android:text="View"
        app:layout_constraintStart_toEndOf="@+id/addClimbs"
        app:layout_constraintTop_toTopOf="@+id/addClimbs" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="98dp"
        android:layout_height="24dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="24dp"
        android:text="Climb Times"
        android:textAlignment="center"
        android:textAllCaps="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/strava_activities_list" />


</androidx.constraintlayout.widget.ConstraintLayout>